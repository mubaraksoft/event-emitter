  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown build src/index.ts --format esm,cjs --dts",
    "dev": "tsdown build src/index.ts --format esm,cjs --dts --watch",
    "prepublishOnly": "npm run build"
  },
  "author": "Abu Bakkar Siddique",
  "description": "Simple libaray to manage events in browser",
  "devDependencies": {
    "tsdown": "^0.15.6",
    "typescript": "^5.9.3"
  }
